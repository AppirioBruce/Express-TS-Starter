{
  "name": "express-ts-starter",
  "version": "1.0.0",
  "description": "A minimal express server starter with TypeScript",
  "main": "server.js",
  "scripts": {
    "start": "node ./build/server.js",
    "start:dev": "nodemon",
    "build": "rm -rf ./build && tsc",
    "test": "jest",
    "test:coverage": "jest --coverage && open coverage/lcov-report/index.html",
    "clean": "rm -rf ./build && rm -rf ./node_modules && rm -rf ./coverage && yarn cache clean && yarn",
    "clean:dev": "yarn clean && yarn start:dev",
    "tslint": "tslint '*.ts'",
    "lint-staged": "lint-staged",
    "semantic-release": "semantic-release",
    "preinstall": "node scripts/preinstall-script.js",
    "postinstall": "yarn build && yarn semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thousight/Express-TS-Starter.git"
  },
  "keywords": [
    "typescript",
    "nodejs",
    "node"
  ],
  "author": "thousight",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thousight/Express-TS-Starter/issues"
  },
  "homepage": "https://github.com/thousight/Express-TS-Starter#readme",
  "dependencies": {
    "@types/compression": "^0.0.36",
    "@types/cors": "^2.8.4",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.0",
    "@types/morgan": "^1.7.35",
    "@types/winston": "^2.4.4",
    "bluebird": "^3.5.3",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "moment": "^2.23.0",
    "morgan": "^1.9.1",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@semantic-release/commit-analyzer": "^6.1.0",
    "@types/jest": "^23.3.12",
    "conventional-changelog-express": "^2.0.1",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
    "nodemon": "^1.18.9",
    "pre-commit": "^1.2.2",
    "prettier": "^1.15.3",
    "semantic-release": "^15.13.3",
    "ts-jest": "^23.10.5",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.1",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.2.2"
  }
}
